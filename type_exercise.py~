doc=raw_input('Please input your document here:')
doc=doc.upper()
len=len(doc)
for line in range(0,len/10):    
    word=doc[line*10:(line+1)*10]
    for a in range(0,5):
        for i in word:
            print voc[i] [a],
        print ''
    print ''
word=doc[len/10*10:len+1]
for a in range(0,5):
    for i in word:
        print voc[i] [a],
    print ''
